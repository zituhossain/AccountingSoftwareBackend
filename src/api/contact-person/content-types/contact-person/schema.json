{
  "kind": "collectionType",
  "collectionName": "contact_people",
  "info": {
    "singularName": "contact-person",
    "pluralName": "contact-people",
    "displayName": "contact_person",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "company_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "email": {
      "type": "string",
      "unique": true
    },
    "address": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "status": {
      "type": "boolean",
      "default": true
    },
    "created_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "contact_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::contact-type.contact-type"
    }
  }
}
