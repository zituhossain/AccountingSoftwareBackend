{
  "kind": "collectionType",
  "collectionName": "quotations",
  "info": {
    "singularName": "quotation",
    "pluralName": "quotations",
    "displayName": "quotation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "quotation_no": {
      "type": "string"
    },
    "subject": {
      "type": "string"
    },
    "supplier_rate": {
      "type": "biginteger",
      "required": true
    },
    "our_rate": {
      "type": "biginteger",
      "required": true
    },
    "no_of_trailers": {
      "type": "integer"
    },
    "overweight": {
      "type": "integer"
    },
    "lc_number": {
      "type": "string"
    },
    "bl_number": {
      "type": "string"
    },
    "remarks": {
      "type": "text"
    },
    "status": {
      "type": "boolean"
    },
    "business_contact_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company"
    },
    "company_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company"
    },
    "quotation_image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": true
    },
    "created_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "send_status": {
      "type": "boolean",
      "default": false
    },
    "revision_count": {
      "type": "integer",
      "default": 0
    }
  }
}
